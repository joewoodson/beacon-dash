{
  "name": "modern-js-stack",
  "version": "1.0.0",
  "description": "project following modern stack tutorial at https://github.com/verekia/js-stack-from-scratch/blob/master/tutorial/",
  "main": "index.js",
  "author": "Joe Woodson",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && NODE_ENV=production webpack -p --progress",
    "prod:start": "heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && flow && jest --coverage",
    "test:mocha": "mocha --compilers js:babel-core/register",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build",
    "heroku-postbuild": "yarn prod:build"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "common-tags": "^1.4.0",
    "compression": "^1.6.2",
    "empty": "^0.10.1",
    "express": "^4.15.2",
    "grommet": "^1.3.4",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.2",
    "react-hot-loader": "next",
    "react-jss": "^5.4.1",
    "react-redux": "^5.0.3",
    "react-router": "next",
    "react-router-dom": "next",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.3",
    "tether": "^1.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "chai-immutable": "^1.6.0",
    "coveralls": "^2.13.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-standard": "^10.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.2.0",
    "fetch-mock": "^5.9.4",
    "flow-bin": "^0.43.1",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "knex": "^0.13.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "pg": "^6.1.5",
    "redux-mock-store": "^1.2.2",
    "rimraf": "^2.6.1",
    "sqlite3": "^3.1.8",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "engines": {
    "yarn": "0.20.3"
  }
}
