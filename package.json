{
  "name": "modern-js-stack",
  "version": "1.0.0",
  "description": "project following modern stack tutorial at https://github.com/verekia/js-stack-from-scratch/blob/master/tutorial/",
  "main": "index.js",
  "author": "Joe Woodson",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon --ignore lib --exec babel-node src/server",
    "prod:build": "rimraf lib && babel src -d lib --ignore .test.js",
    "prod:start": "NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server",
    "test": "eslint src/*.js && flow && jest --coverage",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "dependencies": {
    "common-tags": "^1.4.0",
    "compression": "^1.6.2",
    "express": "^4.15.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-standard": "^10.1.0",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.2.0",
    "flow-bin": "^0.43.1",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "pm2": "^2.4.4",
    "rimraf": "^2.6.1"
  }
}
